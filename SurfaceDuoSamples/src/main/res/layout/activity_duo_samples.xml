<com.microsoft.device.dualscreen.layouts.SurfaceDuoLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/surface_duo_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    app:dual_landscape_single_layout_id="@layout/fragment_app_list"
    app:dual_screen_end_layout_id="@layout/fragment_empty"
    app:dual_screen_start_layout_id="@layout/fragment_app_list"
